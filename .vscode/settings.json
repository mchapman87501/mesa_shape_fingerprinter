{
    "files.associations": {
        "**/templates/*.html": "django-html",
        "**/templates/*": "django-txt",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements",
        "*.html": "html",
        "ios": "cpp",
        "vector": "cpp",
        "*.cmake": "cmake",
        "string": "cpp"
    },
    "[cmake]": {
        "editor.wordBasedSuggestions": "off",
        "editor.quickSuggestions": {
            "other": "on",
            "comments": "off",
            "strings": "off"
        }
    }
}